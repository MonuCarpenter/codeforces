#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/monucarpenter/C++/codeforces/node_modules/.pnpm/mocha@5.2.0/node_modules/mocha/bin/node_modules:/Users/monucarpenter/C++/codeforces/node_modules/.pnpm/mocha@5.2.0/node_modules/mocha/node_modules:/Users/monucarpenter/C++/codeforces/node_modules/.pnpm/mocha@5.2.0/node_modules:/Users/monucarpenter/C++/codeforces/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/monucarpenter/C++/codeforces/node_modules/.pnpm/mocha@5.2.0/node_modules/mocha/bin/node_modules:/Users/monucarpenter/C++/codeforces/node_modules/.pnpm/mocha@5.2.0/node_modules/mocha/node_modules:/Users/monucarpenter/C++/codeforces/node_modules/.pnpm/mocha@5.2.0/node_modules:/Users/monucarpenter/C++/codeforces/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mocha@5.2.0/node_modules/mocha/bin/mocha" "$@"
else
  exec node  "$basedir/../../../../../mocha@5.2.0/node_modules/mocha/bin/mocha" "$@"
fi
